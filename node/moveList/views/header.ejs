  <script>
  
  checkToken();

function checkToken(){
$.ajax({
  url:"/checkToken",
  headers:{   // headers  req.headers 携带 token 发送后端 检测 token 是否 有效 
    token:sessionStorage.token
  },
  success(res){
    console.log(res);
    if(!!res.type){
      $("#login").hide();
      $("#register").hide();
    }else{
      $("#logout").hide();
    }
  }
})
}

  </script>